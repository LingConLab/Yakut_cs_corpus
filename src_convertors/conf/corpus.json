{	
  "corpus_name": "conlab_yakut",
  "corpus_dir": "../corpus/",
  "meta_filename": "meta.csv",
  "speaker_meta_filename": "speaker_meta.json",
  "meta_fields":
  [
    "filename",
    "title",
    "speakers",
    "duration",
    "date"
  ],
  "languages": ["yakut", "inter", "intra"],
  "tier_languages": {
    ".*ЭДЕ.*": "yakut",
    ".*inter.*": "inter",
    ".*intra.*": "intra"
    },
  "main_tiers": [".*ЭДЕ.*"],
  "aligned_tiers": [".*inter.*", ".*intra.*"],
  "analysis_tiers": {
    ".*слова.*": "word",
    ".*мета.*": "gramm",
    ".*перевод.*": "lemma"
  },
  "meta_files_ext": true,
  "meta_files_dir": false,
  "meta_files_case_sensitive": true,
  "cg_disambiguate": false,
  "json_indent": 1,
  "convert_quotes": true,
  "gzip": false,
  "left_quot_mark": "«",
  "right_quot_mark": "»",
  "sent_end_punc": "(?:[.!?;;]+(?:[)\\]}>/»]|$)|\\\\n)",
  "sent_start": "^[A-ZÄÖÜА-ЯЁӤӦӴӞӜ]",
  "newline_ends_sent": true,
  "abbreviations": [],
  "glosses": {
    "yakut": [
    ],
    "russian": [
    ]
  }
}
